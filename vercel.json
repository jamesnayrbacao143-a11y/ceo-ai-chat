{
  "version": 2,
  "rewrites": [
    {
      "source": "/api/cron/cleanup",
      "destination": "/api/cron-cleanup.js"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/server.js"
    }
  ],
  "crons": [
    {
      "path": "/api/cron/cleanup",
      "schedule": "0 2 * * *"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
